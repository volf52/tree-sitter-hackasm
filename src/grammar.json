{
  "name": "hackasm",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_instruction_or_label"
      }
    },
    "_instruction_or_label": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "label_def"
        },
        {
          "type": "SYMBOL",
          "name": "instruction"
        }
      ]
    },
    "label_def": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "SYMBOL",
          "name": "label_ident"
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "instruction": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "a_inst"
        },
        {
          "type": "SYMBOL",
          "name": "c_inst"
        }
      ]
    },
    "a_inst": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "@"
        },
        {
          "type": "SYMBOL",
          "name": "label_ident"
        }
      ]
    },
    "c_inst": {
      "type": "STRING",
      "value": "c"
    },
    "label_ident": {
      "type": "PATTERN",
      "value": "[a-zA-Z]+"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

